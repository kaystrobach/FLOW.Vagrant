<project name="Build">

	<target name="polish" depends="clean, doc-clean" description="Clean in a way that removes all build artifacts"/>

	<target name="clean">
		<delete quiet="true" includeemptydirs="true">
			<fileset refid="files-temporary"/>
		</delete>
	</target>

</project>